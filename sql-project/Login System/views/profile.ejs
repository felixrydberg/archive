<head>
  <meta charset="UTF-8" />
  <meta http-equiv="X-UA-Compatible" content="IE=edge" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <link rel="stylesheet" href="css/style.css" />
  <link rel="stylesheet" href="css/profile.css" />
  <title>Document</title>
</head>
<body>
  <div class="grid-container">
    <header><%- include("include/header.ejs") %></header>
    <main>
      <!-- Show profile etc -->
      <!-- Show Ships -->
      <!-- Add ships -->
      <article class="profile"></article>
      <figure class="userfleet">
        <div class="div-btn">
          <p class="title">Your Fleet:</p>
          <button class="open-addfleet">Add to your fleet</button>
        </div>
        <div class="container"></div>
      </figure>
      <figure class="addships popup">
        <div class="blur">
          <div class="div-btn">
            <button class="close-addfleet">X</button>
          </div>
          <div class="content">
            <div class="box">
              <div class="ship-forms">
                <form class="search">
                  <input
                    type="text"
                    name="name"
                    placeholder="ship-name"
                    required
                  />
                  <button type="submit" class="img">
                    <img src="assets/icons/Search.svg" alt="Search" />
                  </button>
                </form>
                <form class="reset">
                  <button type="submit">Reset</button>
                </form>
              </div>
              <div class="container"></div>
            </div>
          </div>
        </div>
      </figure>
    </main>
    <footer></footer>
  </div>
  <script src="js/profile.js" type="module"></script>
  <script src="js/popup.js" type="module"></script>
</body>
